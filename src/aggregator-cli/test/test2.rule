string message = "";
if (self.Parent != null)
{
    var parent = self.Parent;
    var children = parent.Children;
    if (children.All(c => c.State == "Closed"))
    {
        parent.State = "Closed";
        message = "Parent was closed";
    }
    else
    {
        message = "Parent was already closed";
    }
    parent.Description = parent.Description + " aggregator was here.";
}
return message;
